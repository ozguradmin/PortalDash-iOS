; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Portal Dash"
run/main_scene="res://mainmenu.tscn"
config/features=PackedStringArray("4.5", "GL Compatibility")
boot_splash/bg_color=Color(0, 0, 0, 1)
boot_splash/fullsize=false
boot_splash/image="res://screen.png"
config/icon="res://logo.jpg"
boot_splash/image_scale=1.0

[autoload]

Global="*res://Global.gd"
SkinManager="*res://SkinManager.gd"
VersionManager="*res://VersionManager.gd"
LeaderboardManager="*res://LeaderboardManager.gd"
AdminManager="*res://AdminManager.gd"
SoundManager="*res://SoundManager.gd"

[display]

window/size/viewport_width=390
window/size/viewport_height=844
window/stretch/mode="canvas_items"
window/handheld/orientation=1

[rendering]

textures/canvas_textures/default_texture_filter=0
renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
textures/vram_compression/import_etc2_astc=true
